<?xml version="1.0" encoding="utf-8"?>
<manifest package="alvin.anglesearch"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/angle"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.DimensionActivity"
            android:label="@string/title_activity_add_dimension">
        </activity>
        <activity
            android:name=".activity.BulkActivity"
            android:label="@string/title_activity_add_bulk"
            android:theme="@style/Theme.AppCompat.Light.Dialog">
        </activity>
        <activity android:name=".activity.AngleActivity">
        </activity>
        <activity
            android:name=".activity.SearchActivity"
            android:label="@string/title_activity_search">
        </activity>
    </application>

</manifest>
